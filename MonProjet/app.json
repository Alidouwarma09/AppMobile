{
  "expo": {
    "name": "MonProjet",
    "slug": "MonProjet",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fff",
      "image": "./assets/ic_launcher.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
